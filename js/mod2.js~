function imgMdLoad1x() {
      $("#imgMdLoad").html("<img src='img/bnr/Screenshot from 2015-10-14 20:44:35b.png'>");
      $("#txtMdLoad1a").text('Screenshot from my computer');
      $('.imgMdCnt').show(300);
      $('.bckDrp').show();
}

